go() {
  requested=$1
  for project in $(go_projects_list)
  do
    if [[ $requested = $(basename $project) ]]
    then
      goto_project $project
      return
    fi
  done
}

go_projects_list() {
  find ~/Projects -type d -d 2
}

goto_project() {
  echo "cd $1/source"
  builtin cd $1/source
}

compctl -k "($(go_projects_list | xargs basename))" go
