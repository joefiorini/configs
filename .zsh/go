go() {
  requested=$1
  for project in $(go_projects_list)
  do
    if [[ $requested = $(basename $project) ]]
    then
      goto_project "$project"
      return
    fi
  done
}

go_projects_list() {
  find -L ~/Projects -type d -d 2
}

goto_project() {
  local godir
  if [ -d "$1/source" ]; then
    godir="$1/source"
  else
    godir=$1
  fi
  echo "cd $godir"
  builtin cd $godir
}
